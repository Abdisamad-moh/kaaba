        <div class="col-12">
            <h5 class="fs-4 fw-semibold mb-4">More Details</h5>
        </div>

        <div class="col-6">
            <label class="form-label">Company Name</label>
            <p class="form-control-plaintext">{{ tender.companyName }}</p>
        </div>

        <div class="col-6">
            <label class="form-label">Contact Name</label>
            <p class="form-control-plaintext">{{ tender.contactName }}</p>
        </div>

        <div class="col-6">
            <label class="form-label">Contact Email</label>
            <p class="form-control-plaintext">{{ tender.contactEmail }}</p>
        </div>

        <div class="col-6">
            <label class="form-label">Contact Phone</label>
            <p class="form-control-plaintext">{{ tender.contactPhone }}</p>
        </div>

        <div class="col-3">
            <label class="form-label">Country</label>
            <p class="form-control-plaintext">{{ tender.country.name }}</p>
        </div>

        <div class="col-3">
            <label class="form-label">City</label>
            <p class="form-control-plaintext">{{ tender.city.name }}</p>
        </div>

        <div class="col-3">
            <label class="form-label">State</label>
            <p class="form-control-plaintext">{{ tender.states ? tender.states.name : '—' }}</p>
        </div>

        <div class="col-3">
            <label class="form-label">ZIP / Postal Code</label>
            <p class="form-control-plaintext">{{ tender.zip }}</p>
        </div>

        <div class="col-6">
            <label class="form-label">External Link</label>
            {% if tender.externalLink %}
                <p class="form-control-plaintext">
                    <a href="{{ tender.externalLink }}" target="_blank">{{ tender.externalLink }}</a>
                </p>
            {% else %}
                <p class="form-control-plaintext">—</p>
            {% endif %}
        </div>

        <div class="col-6">
            <label class="form-label">Tender File</label>
            {% if tender.attachment %}
                <p class="form-control-plaintext">
                    <a href="{{ asset('uploads/tenders/' ~ tender.attachment) }}" target="_blank">
                        {{ tender.attachment }}
                    </a>
                </p>
            {% else %}
                <p class="form-control-plaintext">—</p>
            {% endif %}
        </div>
    </div>

