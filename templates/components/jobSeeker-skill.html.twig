<div {{ attributes }}>
    {{ form_start(form, {attr: {
        'data-action': 'live#action:prevent',
        'data-live-action-param': 'save'
    }}) }}
        <div class="modal-body"> 
            {{ form_row(form.jobSeekerWorks) }}
        {# {{ dump(_errors ?? '') }} #}
        </div>
        <div class="d-flex justify-content-end gap-2">
            <button  class="btn btn-primary {{ this.hasValidationErrors ? 'disabled' : '' }}">Save</button>
        </div>
    {{ form_end(form) }}
</div>